curl -i -H "Content-Type: application/json" -X POST -d '{"address": {"country": "Russia", "town": "Barnaul"}, "end_time": 15, "seller": {"login": "user3"}, "start_price": {"currency": "EUR", "value": 12}, "start_time": 5}' http://localhost:5000/items

curl -i -H "Content-Type: application/json" -X PUT -d '{"address": {"country": "USA", "town": "New York"}, "end_time": 30, "seller": {"login": "put_user"}, "start_price": {"currency": "CNY", "value": 120}, "start_time": 13}' http://localhost:5000/items/2

curl -i -H "Content-Type: application/json" -X DELETE http://localhost:5000/items/2

curl -i 127.0.0.1:5000/items/2

curl -i -H "Content-Type: application/json" -X POST -d '{"currency": "EUR", "value": 12, "new_currency": "USD"}' http://localhost:5000/convert_price

curl -i -H "Content-Type: application/json" -X POST -d '{"price_from": 4, "price_to": 10}' http://localhost:5000/search_items

http://blog.miguelgrinberg.com/post/designing-a-restful-api-with-python-and-flask
